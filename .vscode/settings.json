{
    "files.exclude": {
        ".history": true
    },
    "eslint.autoFixOnSave": true,
    "debug.node.autoAttach": "on",
    "editor.tabSize": 2,
    "editor.autoIndent": true,
    "workbench.panel.defaultLocation": "right",
    "liveServer.settings.port": 5501,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    }
}